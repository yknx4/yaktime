    "@types/node": "^10.12.21",
    "@types/restify-errors": "^4.3.3",
    "@types/rimraf": "^2.0.2",
    "@types/supertest": "^2.0.7",
    "@types/xxhashjs": "^0.2.0",
    "babel-jest": "^24.1.0",
    "barrelsby": "^1.0.2",
    "codecov": "^3.1.0",
    "expect-more-jest": "^2.4.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-junit": "^6.2.1",
    "jest-runner-tsc": "^1.4.0",
    "lint-staged": "^8.1.3",
